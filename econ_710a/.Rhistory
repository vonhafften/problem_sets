q <- seq(from = .001, to = 10, by = .001)
elasticity <- q
k <- 10
# constant elasticity supply curve
p <- (q/k)^(1/elasticity)
plot(1, type = "n", xlim = c(0, 10), ylim = c(0, 10), xlab = "Q", ylab = "P")
lines(x=q, y=p)
